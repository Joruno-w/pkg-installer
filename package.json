{
  "name": "pkg-installer",
  "version": "0.0.4",
  "packageManager": "pnpm@7.12.0",
  "description": "Install package programmatically.",
  "author": "Joruno-w",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Joruno-w",
  "homepage": "https://github.com/antfu/pkg-installer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/pkg-installer.git"
  },
  "bugs": {
    "url": "https://github.com/antfu/pkg-installer/issues"
  },
  "keywords": [
    "scripts",
    "installer",
    "Install",
    "pkg",
    "package"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "nr build",
    "dev": "nr build --watch",
    "start": "esno src/index.ts",
    "build": "tsup src/index.ts --format cjs,esm --dts --no-splitting",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "lint": "eslint ."
  },
  "dependencies": {
    "execa": "^5.1.1",
    "find-up": "^5.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@antfu/ni": "^0.18.0",
    "@types/node": "^16.11.59",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.1",
    "esno": "^0.16.3",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  }
}
